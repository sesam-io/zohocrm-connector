[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "add_namespaces": false,
    "metadata": {},
    "source": {
      "id_expression": "{{ id }}",
      "is_chronological": true,
      "operation": "list-records",
      "payload_property": "data",
      "properties": {
        "fields": "Account_Name,Assistant,Asst_Phone,Created_By,Created_Time,Date_of_Birth,Department,Description,Email,Email_Opt_Out,Enrich_Status__s,Fax,First_Name,Full_Name,Home_Phone,Last_Activity_Time,Last_Enriched_Time__s,Last_Name,Lead_Source,Mailing_City,Mailing_Country,Mailing_State,Mailing_Street,Mailing_Zip,Mobile,Modified_By,Modified_Time,Other_City,Other_Country,Other_Phone,Other_State,Other_Street,Other_Zip,Owner,Phone,Record_Image,Reporting_To,Salutation,Secondary_Email,Skype_ID,Title,Twitter,Unsubscribed_Mode,Unsubscribed_Time",
        "module": "Contacts"
      },
      "supports_since": true,
      "system": "{{@ system @}}",
      "trace": {
        "log_request_body_maxsize": 100000,
        "log_response_body_maxsize": 100000
      },
      "type": "rest"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "$last-modified",
              [
                "datetime-parse",
                "%Y-%m-%dT%H:%M:%S%z",
                "_S.Modified_Time"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "primary_key": "id",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-collect-rest",
        "type": "template"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-share",
    "batch_size": 1,
    "namespaced_identifiers": false,
    "sink": {
      "deletion_tracking": false,
      "set_initial_offset": "onload"
    },
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-transform",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "create-payload": [
            [
              "copy",
              "*",
              [
                "list",
                "_*",
                "$*"
              ]
            ],
            ["if",
              ["is-empty", "_S.id"],
              ["remove", "id"]
            ]
          ],
          "default": [
            [
              "copy",
              "*"
            ],
            [
              "add",
              "$payload",
              [
                "dict",
                "data",
                [
                  "list",
                  [
                    "apply",
                    "create-payload",
                    "_S."
                  ]
                ]
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "properties": {
          "module": "Deals",
          "operation_delete": "{{@ datatype @}}-delete",
          "operation_insert": "{{@ datatype @}}-upsert",
          "operation_lookup": "{{@ datatype @}}-lookup",
          "operation_update": "{{@ datatype @}}-upsert",
          "primary_key": "id",
          "primary_key_insert": "details.id",
          "rest_system": "{{@ system @}}",
          "share_dataset": "{{@ system @}}-{{@ datatype @}}-share"
        },
        "template": "transform-share-rest",
        "trace": {
          "log_request_body_maxsize": 100000,
          "log_response_body_maxsize": 100000
        },
        "type": "template"
      }
    ],
    "type": "pipe"
  }
]